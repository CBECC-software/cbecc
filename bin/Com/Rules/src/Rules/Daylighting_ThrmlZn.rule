// the following four rules identify the Daylit Zone type that each reference point 
// will be associated with
RULE NEW  ThrmlZn:DayltgCtrl1ZnType
  DESCRIPTION
    ""   
  REFERENCE 

  DATATYPE
    STRING
  LONGFORM
    DaylightingControl1ZoneType
  INPUTCLASS
    NotInput
  DEFAULT
    if ( SecSideDaylitArea > 0 ) then
      if( SkylitDaylitArea > 0 .AND. PriSideDaylitArea > 0 ) 
      then "Skylit + PriSide" else 
      if ( SkylitDaylitArea > 0 )
      then "Skylit" else
      if ( PriSideDaylitArea > 0 )
      then "PriSide" else 
      "SecSide"  
      endif endif endif
    else 
      if ( SkylitDaylitArea > 0 ) 
      then "Skylit" else
      if ( PriSideDaylitArea > 0 )
      then "PriSide" else
      "None"
      endif endif
    endif
  SIZING
    "None"
  ANNUAL
          if ( SecSideDaylitArea > 0 ) then
      if( SkylitDaylitArea > 0 .AND. u:PriSideDaylitArea > 0 ) 
      then "Skylit + PriSide" else 
      if ( SkylitDaylitArea > 0 )
      then "Skylit" else
      if ( PriSideDaylitArea > 0 )
      then "PriSide" else 
      "SecSide"  
      endif endif endif
    else 
      if ( SkylitDaylitArea > 0 ) 
      then "Skylit" else
      if ( PriSideDaylitArea > 0 )
      then "PriSide" else
      "None"
      endif endif
    endif
ENDRULE
RULE NEW  ThrmlZn:DayltgCtrl2ZnType
  DESCRIPTION
    ""   
  REFERENCE 

  DATATYPE
    STRING
  LONGFORM
    DaylightingControl1ZoneType
  INPUTCLASS
    NotInput
  DEFAULT
    if ( SecSideDaylitArea > 0 ) then
      if ( DaylightingControl1ZoneType = "SecSide" ) then
      "None" else
      "SecSide"
      endif
    else
      if ( DaylightingControl1ZoneType = "Skylit" ) then
        if ( PriSideDaylitArea > 0 ) then
        "PriSide" else
        "None"
        endif
      else "None"
      endif
    endif
  SIZING
    "None"
  ANNUAL
    if ( SecSideDaylitArea > 0 ) then
      if ( DaylightingControl1ZoneType = "SecSide" ) then
      "None" else
      "SecSide"
      endif
    else
      if ( DaylightingControl1ZoneType = "Skylit" ) then
        if ( PriSideDaylitArea > 0 ) then
        "PriSide" else
        "None"
        endif
      else "None"
      endif
    endif
ENDRULE
RULE NEW  ThrmlZn:DayltgCtrl1Cat
  DESCRIPTION
    ""   
  REFERENCE 

  DATATYPE
    STRING
  LONGFORM
    DaylightingControl1Category
  INPUTCLASS
    NotInput
  DEFAULT
    if ( DayltgCtrl1ZnType = "Skylit + PriSide" ) then
      if ( PriSideMaxRDP > SkylitMaxRDP ) then
      "PriSide" else
      "Skylit"
      endif
    else DayltgCtrl1ZnType
    endif
 SIZING
    "None"
  ANNUAL
    if ( DayltgCtrl1ZnType = "Skylit + PriSide" ) then
      if ( PriSideMaxRDP > SkylitMaxRDP ) then
      "PriSide" else
      "Skylit"
      endif
    else DayltgCtrl1ZnType
    endif
ENDRULE
