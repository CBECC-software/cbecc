RULE ThrmlZn:DayltgCtrlLtgFrac1
  DESCRIPTION
    "Fraction of Total Power of all controlled lighting controlled by Reference Point 1"   
  INPUTCLASS
    Optional
  MINIMUM 
    0 
  MAXIMUM 
    1
  DEFAULT
    if ( DayltgCtrlLtgPwr1 = 0 ) 
    then 0 else
    if ( RegLtgPwr > 0 ) then MIN( 1,DayltgCtrlLtgPwr1 / RegLtgPwr ) 
    else 0
    endif endif
  SIZING
    0
  ANNUAL
    if ( DayltgCtrlLtgPwr1 = 0 ) then 0 else
    if ( RegLtgPwr > 0 ) then MIN( 1,DayltgCtrlLtgPwr1 / RegLtgPwr )
    else 0
    endif endif
ENDRULE
RULE ThrmlZn:DayltgCtrlLtgFrac2
  DESCRIPTION
    "Fraction of Total Power of all controlled lighting controlled by Reference Point 2"   
  INPUTCLASS
    Optional
  MINIMUM 
    0 
  MAXIMUM 
    1
  DEFAULT
    if ( DayltgCtrlLtgPwr2 = 0 ) then 0 else
    if ( RegLtgPwr > 0 ) then 
      MIN( (0.99999-DayltgCtrlLtgFrac1 ),DayltgCtrlLtgPwr2 / RegLtgPwr ) 
    else 0
    endif endif
  SIZING
    0
  ANNUAL
    if ( DayltgCtrlLtgPwr2 = 0 ) then 0 else
    if ( RegLtgPwr > 0 ) then 
      MIN( (0.99999-DayltgCtrlLtgFrac1 ),DayltgCtrlLtgPwr2 / RegLtgPwr ) 
    else 0
    endif endif
ENDRULE

