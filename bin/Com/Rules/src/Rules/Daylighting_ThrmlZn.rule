RULE NEW  ThrmlZn:DayltgCtrlLtgPwr1
  DESCRIPTION
    "Total Power of all controlled lighting controlled by Reference Point 1"   
  DATATYPE
    Float
  LONGFORM
    DaylightingControlLightingPower1
  INPUTCLASS
    NotInput
  UNITS
    Watts
  MINIMUM 
    0 
  MAXIMUM 
   1000000
  DEFAULT
    if ( IfValidAnd( Proj:DisableDayltgCtrls = 1 )) then 0 else  
    if ( DayltgCtrl1ZnType = "Skylit + PriSide" ) then
      SkylitDayltgCtrlLtgPwr + PriSideDayltgCtrlLtgPwr else
    if ( DayltgCtrl1ZnType = "Skylit" ) then
      SkylitDayltgCtrlLtgPwr else
    if ( DayltgCtrl1ZnType = "PriSide" ) then
      PriSideDayltgCtrlLtgPwr else
    if ( DayltgCtrl1ZnType = "SecSide" ) then
      SecSideDayltgCtrlLtgPwr else
    0  
    endif endif endif endif endif
  SIZING
    0  
  ANNUAL
    if ( IfValidAnd( u:Proj:DisableDayltgCtrls = 1 )) then 0 else 
    if ( DayltgCtrl1ZnType = "Skylit + PriSide" ) then
        SkylitDayltgCtrlLtgPwr + PriSideDayltgCtrlLtgPwr else
    if ( DayltgCtrl1ZnType = "Skylit" ) then
        SkylitDayltgCtrlLtgPwr else
    if ( DayltgCtrl1ZnType = "PriSide" ) then
        PriSideDayltgCtrlLtgPwr else
    if ( DayltgCtrl1ZnType = "SecSide" ) then
        SecSideDayltgCtrlLtgPwr else
    0  
    endif endif endif endif endif
ENDRULE
