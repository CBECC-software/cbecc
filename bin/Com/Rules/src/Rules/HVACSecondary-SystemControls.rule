// ********** Optimum Start Control ********************************************
RULE AirSys:OptStart
  DESCRIPTION
    "The control capability to adjust scheduling of system start time based on 
     building loads."
  REFERENCE 
    NACM Section 5.7.2.2
  INPUTCLASS : T24N_2013
    NotInput
  INPUTCLASS
    Default
  DEFAULT
    0
  CHECKCODE : S901G ECBC
    if( IfValidAnd(OptStart = 0) .AND. IfValidAnd(SupFanCap > 10000) )
    then 
      PostError("AirSystem '%s' has Supply Fan Capacity greater than 10,000 cfm
                 and therefore must have Optimum Start Control.", Name)
    else UNCHANGED
    endif
  CHECKCODE : T24N_2016
    if( IsNew .AND. OptStart = 0 .AND. IsMultiZnSys .AND. 
        IfValidAnd( CtrlSysType = "DDCToZone" ) )
    then 
      PostError("AirSystem '%s' has DDCToZone control. OptimumStart is 
                 required.", Name)
    else UNCHANGED
    endif
  SIZING
    0 // OptStart not used for sizing
  ANNUAL : S901G ECBC
    if( BaseSysNum > 0 )
    then
      if( IfValidAnd(SupFanCap > 10000) ) then 1 else 0 endif
    else u:OptStart
    endif
  ANNUAL : T24N_2016
    if( BaseSysNum > 0 )
    then
      if( IsMultiZnSys .AND. IfValidAnd( CtrlSysType = "DDCToZone" ) )
      then 1
      else 0
      endif
    else u:OptStart
    endif
  ANNUAL : T24N_2013
    0  // OptStart not applicable
ENDRULE
RULE ZnSys:OptStart
  DESCRIPTION
    "The control capability to adjust scheduling of system start time based on 
     building loads."
  REFERENCE 
    NACM Section 5.7.2.2
  INPUTCLASS : T24N
    NotInput   IgnoreUserInput  "ZnSys:OptStart removed in transition from 2013 3d to 3e and 2016.1 to 2016.2"
  INPUTCLASS : S901G
    Default
  DEFAULT
    0
  CHECKCODE : S901G
    if( IfValidAnd(OptStart = 0) .AND. IfValidAnd(SupFanCap > 10000) )
    then 
      PostError("Zone System '%s' has Supply Fan Capacity greater than 10,000 cfm
                 and therefore must have Optimum Start Control.", Name)
    else UNCHANGED
    endif
  SIZING
    0 // OptStart not used for sizing
  ANNUAL : S901G
    if( BaseSysNum > 0 )
    then
      if( IfValidAnd(SupFanCap > 10000) ) then 1 else 0 endif
    else u:OptStart
    endif
  ANNUAL
    0 // OptStart not supported for ZnSys
ENDRULE
// Optimum Start Control
RULE AirSys:OptStartCtrl
  DESCRIPTION
    "The control capability to adjust scheduling of system start time based on 
     building loads."
  REFERENCE 
    NACM Section 5.7.2.2
  INPUTCLASS : T24N
    NotInput
  INPUTCLASS : S901G
    Default
  OPTION
    None
    ControlZone
    AnyZone
  DEFAULT
    if( OptStart = 1 ) 
    then
      if( IsMultiZnSys .AND. IfValidAnd( CtrlSysType = "DDCToZone"  ) )
      then "AnyZone"
      else if( IsSglZnSys .AND. IsExhSys = 0 )
      then "ControlZone"
      else "None"
      endif endif
    else "None"
    endif
  SIZING
   "None"  // Optimum start not used for sizing
  ANNUAL : S901G
    if( BaseSysNum > 0 )
    then
      if( IfValidAnd(OptStart = 1) )
      then
        if( IfValidAnd(Type = "PVAV") .OR. IfValidAnd(Type = "VAV") )
        then "AnyZone"
        else if( IfValidAnd(IsExhSys != 0) )
        then UNDEFINED
        else "ControlZone"
        endif endif
      else "None"
      endif
    else if (LocalStatus( u:OptStartCtrl ) > 0)
    then
      u:OptStartCtrl
    else  UNDEFINED
    endif endif
  ANNUAL : T24N_2013
    "None" // OptStart not applicable
  ANNUAL : T24N_2016
    if( BaseSysNum > 0 )
    then
      if( OptStart = 1 ) 
      then
        if( IsMultiZnSys .AND. IfValidAnd( CtrlSysType = "DDCToZone"  ) )
        then "AnyZone"
        else if( IsSglZnSys .AND. IsExhSys = 0 )
        then "ControlZone"
