RULE AirSys:OptStartRpt
  DESCRIPTION
    "A text string if Optimum Start is selected for the proposed model."
  INPUTCLASS 
    NotInput
  ANNUAL_PROPOSED
    if( IfValidAnd(OptStart = 1) )
    then "Optimum Start"
    else
     if( IfValidAnd(OptStart = 0) )
     then "No Optimum Start"
     else UNDEFINED
     endif
    endif
ENDRULE

