// ********** Cooling Supply Air Temperature ***********************************
// =========================== AirSystem ======================================
RULE AirSys:ClgDsgnSupAirTemp
  DESCRIPTION
    "The design cooling supply air temperature of single duct system or the cold
     duct of a dual duct system. This is also the minimum supply air temperature
     setpoint for SZVAV systems in cooling mode."
  REFERENCE 
    NACM Section 5.7.2.3    
  INPUTCLASS
    Default
  COMMONMINIMUM
    50
  COMMONMAXIMUM
    80
  UNITS 
    °F
  DEFAULT
    if( IfValidAnd( ClgCap = 0 ) )
    then UNDEFINED
    else if( SubType = "CRAC" .OR. SubType = "CRAH" )
    then 60
    else Max( 55, MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp ) )
    endif endif
  SIZING
    if( BaseSysNum > 0 )
    then
      if( SubType = "CRAC" .OR. SubType = "CRAH" )
      then 60
      else 
      switch ( Type )
        case "SZAC"      :  MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp)
        case "SZHP"      :  MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp)
        case "PVAV"      :  MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp)
        case "VAV"       :  MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp)
        case "SZVAVAC"   :  MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp)
        case "SZVAVHP"   :  MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp)
        case "HV"        :  UNDEFINED
        default : UNDEFINED
      endswitch
      endif
    else if( LocalStatus( u:ClgDsgnSupAirTemp ) > 0 )
    then u:ClgDsgnSupAirTemp
    else if( IfValidAnd( ClgCap > 0 ) )
    then Max( 55, MinRevRef(ThrmlZn:PriAirCondgSysRef, ThrmlZn:ClgDsgnSupAirTemp ) )
    else UNDEFINED
    endif endif endif
  ANNUAL 
    z:ClgDsgnSupAirTemp
ENDRULE
