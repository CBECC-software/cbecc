RULE NEW ThrmlZn:FullDuctRetFanPwrAdj
  DATATYPE
    Float
  LONGFORM
    FullyDuctedReturnFanPowerAdjustment
  DESCRIPTION
    ""  
  HELP
    ""
  UNITS
    BHP
  INPUTCLASS
    NotInput
  SIZING_PROPOSED
    if( IsCond .AND. LocalCompAssigned( PriAirCondgSysRef ) )
    then
      if( UniqueVentSysAssigned = 1 )
      then
        ( ( PriAirCondgSysRef:FullDuctRetPDAdj *
            PriAirCondgSysRef:FullDuctRetDevFlow *
            PriZnToSysFlowRat ) +
          ( VentSysRef:FullDuctRetPDAdj *
            VentSysRef:FullDuctRetDevFlow *
            VentZnToSysFlowRat ) ) / 4131
      else
        ( PriAirCondgSysRef:FullDuctRetPDAdj *
          PriAirCondgSysRef:FullDuctRetDevFlow *
          PriZnToSysFlowRat ) / 4131
      endif
    else 0
    endif       
  SIZING_BASELINE
    zp:FullDuctRetFanPwrAdj
  ANNUAL_BASELINE
    FullDuctRetFanPwrAdj * BaseToPropDsgnFlowRat
ENDRULE
