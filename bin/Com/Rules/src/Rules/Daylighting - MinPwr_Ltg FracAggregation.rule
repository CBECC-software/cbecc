// ---------- Light Heat Gain Distribution - Luminaire ------------------------

RULE NEW Lum:MinDimPwrFrac
  DESCRIPTION
     "The minimum power fraction when controlled lighting is fully dimmed. 
      Minimum power fraction = (Minimum power) / (Full rated power)."  
  REFERENCE 
     ACM 5.5.5 - Daylighting, Minimum Dimming Power Fraction
  LONGFORM
    MinimumDimmingPowerFraction
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  MINIMUM 
    0
  MAXIMUM 
    1
  DEFAULT
    DayltCtrlParms:MinDimPwrFrac( "LampType",u:LampType )
  SIZING
    UNDEFINED
  ANNUAL
    UNDEFINED
ENDRULE
RULE NEW Lum:MinDimLtgFrac
  DESCRIPTION
     "Minimum light output of controlled lighting when fully dimmed. 
      Minimum light fraction = (Minimum light output) / (Rated light output)."     
  REFERENCE 
     ACM 5.5.5 - Daylighting, Minimum Dimming Light Fraction
  LONGFORM
    MinimumDimmingLightFraction
  DATATYPE
    Float
  INPUTCLASS
    NotInput
  MINIMUM 
    0
  MAXIMUM 
    1
  DEFAULT
    DayltCtrlParms:MinDimLtgFrac( "LampType",u:LampType )
  SIZING
    UNDEFINED
  ANNUAL
    UNDEFINED
ENDRULE

// ---------- Light Heat Gain Distribution - IntLtgSys ------------------------
RULE NEW IntLtgSys:LumMinDimPwrFracXPwr1
   DATATYPE
     Float
   MINIMUM 
     0
   INPUTCLASS
     NotInput
   DEFAULT
		if ( LumRefPresent1 < 0.5 ) then 0 
		else LumRef[1]:MinDimPwrFrac * LumRef[1]:Pwr * LumCnt[1] endif
  SIZING
    UNDEFINED
  ANNUAL
    UNDEFINED
ENDRULE
