// ********** TableHardSizing **************************************************
RULE NEW Proj:ModelName
  DATATYPE
    String
  LONGFORM
    ModelName
  INPUTCLASS
    NotInput
  DESCRIPTION
    "An intermediate model name used for TableSizing 0X00 model HVAC system capacities."
  DEFAULT
    switch ( Name )
      case "010012-SchSml-CECStd13"       : "SchSmlCECStd13010012"
      case "020012-OffSml-CECStd13"       : "OffSmlCECStd13020012"
      case "020012S-OffSml-CECStd13"      : "OffSmlCECStd13020012S"
      case "030012-OffMed-CECStd13"       : "OffMedCECStd13030012"
      case "030012S-OffMed-CECStd13"       : "OffMedCECStd13030012S"
      case "040012-OffLrg-CECStd13"       : "OffLrgCECStd13040012"
      case "050012-RetlMed-CECStd13"      : "RetlMedCECStd13050012"
      case "060012-RstntSml-CECStd13"     : "RstntSmlCECStd13060012"
      case "070012-HotSml-CECStd13"       : "HotSmlCECStd13070012"
      case "080012-Whse-CECStd13"         : "WhseCECStd13080012"
      case "090012-RetlLrg-CECStd13"      : "RetlLrgCECStd13090012"
; T24N_2016 models
      case "010012-SchSml-CECStd16"       : "SchSmlCECStd16010012"
      case "020012-OffSml-CECStd16"       : "OffSmlCECStd16020012"
      case "020012S-OffSml-CECStd16"      : "OffSmlCECStd16020012S"
      case "030012-OffMed-CECStd16"       : "OffMedCECStd16030012"
      case "030012S-OffMed-CECStd16"      : "OffMedCECStd16030012S"
      case "040012-OffLrg-CECStd16"       : "OffLrgCECStd16040012"
      case "050012-RetlMed-CECStd16"      : "RetlMedCECStd16050012"
      case "060012-RstntSml-CECStd16"     : "RstntSmlCECStd16060012"
      case "070012-HotSml-CECStd16"       : "HotSmlCECStd16070012"
      case "080012-Whse-CECStd16"         : "WhseCECStd16080012"
      case "090012-RetlLrg-CECStd16"      : "RetlLrgCECStd16090012"
; S901G models
      case "020032-OffSml-S901GElectric"  : "OffSmlS901GElectric020032"
      case "020032-OffSml-S901GFossil"    : "OffSmlS901GFossil020032"
      case "030032-OffMed-S901GElectric"  : "OffMedS901GElectric030032"
      case "070032-HotSml-S901GElectric"  : "HotSmlS901GElectric070032"
      case "080032-Whse-S901GFossil"      : "WhseS901GFossil080032"
