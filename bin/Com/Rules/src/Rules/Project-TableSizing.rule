RULE NEW Proj:IsStdModel
  DATATYPE
    Integer
  LONGFORM
    IsStandardModel
  INPUTCLASS
    NotInput
  DESCRIPTION
    "An integer indicating the model is a Standard model."
  DEFAULT
    if( ModelName != "" )
    then 1
    else 0
    endif
ENDRULE

