          ( Type = "DirectExpansion" .AND. CndsrType = "WaterSource" ) ) .AND.
        Proj:AutoHardSize = 1 .AND.
        Proj:Name != "" )
    then
      switch ( Proj:Name ) 
; T24N_2013
        case "010012-SchSml-CECStd13" : 
          SchSmlCECStd13010012CoilClg:FluidFlowRtDsgn( "PropName", Name ) 
        case "020012-OffSml-CECStd13" : 
          OffSmlCECStd13020012CoilClg:FluidFlowRtDsgn( "PropName", Name ) 
