  DEFAULT
    if( IfValidAnd( NonResExtRoofArea > 0 ) .AND.
        IfValidAnd( NonResExtSkyltArea > 0 ) )
    then  NonResExtSkyltArea / NonResExtRoofArea
    else  0
    endif
  SIZING  //  Recalculate in case window size is adjusted in either prop or base transform
    if( IfValidAnd( NonResExtRoofArea > 0 ) .AND.
        IfValidAnd( NonResExtSkyltArea > 0 ) )
    then  NonResExtSkyltArea / NonResExtRoofArea
    else  0
    endif
  ANNUAL  
    z:NonResSRR
ENDRULE
