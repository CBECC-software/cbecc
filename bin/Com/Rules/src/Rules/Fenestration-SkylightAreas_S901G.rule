  DATATYPE
    Float
  LONGFORM
    SkylightArea
  DESCRIPTION
    "Area of all Skylights on the Exterior Roof"  
  HELP
    ""  
  REFERENCE 
    
  INPUTCLASS
    NotInput       
  DEFAULT
    SumChildren( Skylt:Area )
  SIZING_PROPOSED
