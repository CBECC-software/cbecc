RULE NEW Bldg:ResSRRCat
  DATATYPE
    String
  LONGFORM
    ResidentialSkylightRoofRatioCategory
  DESCRIPTION
    ""  
  HELP
    ""  
  REFERENCE 
    
  INPUTCLASS
    NotInput  
  DEFAULT
    if(LocalStatus(ResSRR) = 0) then UNCHANGED
    else if(ResSRR <= 0.20) then "LTE 0.2"
    else if(ResSRR > 0.20)  then "GT 0.2"
    else UNCHANGED
    endif endif endif  
  SIZING
    if(LocalStatus(ResSRR) = 0) then UNCHANGED
    else if(ResSRR <= 0.20) then "LTE 0.2"
    else if(ResSRR > 0.20)  then "GT 0.2"
    else UNCHANGED
    endif endif endif  
  ANNUAL
    z:ResSRRCat
ENDRULE
