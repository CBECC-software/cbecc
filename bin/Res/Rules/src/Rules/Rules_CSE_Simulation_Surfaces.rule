;*************************************************************************
RULELIST "CAResAttic_CreateTrussMatAndCons" 0 0 0 0
;*************************************************************************
	"Blast attic truss material & construction components"	Attic:Action = {	EvalRulelist( "CAResAttic_BlastTrussMatAndCons", 1 )  }    ; should not be necessary, but just in case...

	"Create Attic:TrussMATERIAL"  		Attic:TrussMATERIAL				= {	CreateComp( cseMATERIAL,     "m-AtticTruss" )  }
;	"Set Attic:TrussMATERIAL:matCond"  	Attic:TrussMATERIAL:matCond	= {	0.0667  }
;	"Set Attic:TrussMATERIAL:matSpHt"  	Attic:TrussMATERIAL:matSpHt	= {	0.33  }
;	"Set Attic:TrussMATERIAL:matDens"  	Attic:TrussMATERIAL:matDens	= {	32.0  }
	"Set Attic:TrussMATERIAL:matCond"  	Attic:TrussMATERIAL:matCond	= {	if (GlobalCompAssigned( Proj:LibMat_SoftWood ) < 1) then  UNDEFINED
																									else if (GlobalStatus( Proj:LibMat_SoftWood:ConductivityQII ) < 1) then  UNDEFINED
																									else  GlobalRef( Proj:LibMat_SoftWood:ConductivityQII )  endif endif  }
	"Set Attic:TrussMATERIAL:matCondCT"	Attic:TrussMATERIAL:matCondCT	= {	if (GlobalCompAssigned( Proj:LibMat_SoftWood ) < 1) then  UNDEFINED
																									else if (GlobalStatus( Proj:LibMat_SoftWood:ConductivityCT ) < 1) then  UNDEFINED
																									else  GlobalRef( Proj:LibMat_SoftWood:ConductivityCT )  endif endif  }
	"Set Attic:TrussMATERIAL:matSpHt"  	Attic:TrussMATERIAL:matSpHt	= {	if (GlobalCompAssigned( Proj:LibMat_SoftWood ) < 1) then  UNDEFINED
																									else if (GlobalStatus( Proj:LibMat_SoftWood:SpecHeat ) < 1) then  UNDEFINED
																									else  GlobalRef( Proj:LibMat_SoftWood:SpecHeat )  endif endif  }
	"Set Attic:TrussMATERIAL:matDens"  	Attic:TrussMATERIAL:matDens	= {	if (GlobalCompAssigned( Proj:LibMat_SoftWood ) < 1) then  UNDEFINED
																									else if (GlobalStatus( Proj:LibMat_SoftWood:Density ) < 1) then  UNDEFINED
																									else  GlobalRef( Proj:LibMat_SoftWood:Density )  endif endif  }

	"Create Attic:TrussCONS"				Attic:TrussCONS					= {	CreateComp( cseCONSTRUCTION, "con-AtticTruss" )  }

	"Create Attic:TrussLAYER"				Attic:TrussLAYER					= {	CreateComp( cseLAYER,        "lyr-AtticTruss" )  }
	"Set Attic:TrussLAYER:Parent"  		Attic:TrussLAYER:Parent			= {	Local( TrussCONS )  }
	"Set Attic:TrussLAYER:lrMat"  		Attic:TrussLAYER:lrMat			= {	Local( TrussMATERIAL )  }
	"Set Attic:TrussLAYER:lrThk"  		Attic:TrussLAYER:lrThk			= {	0.75 / 12  }
END
;*************************************************************************
RULELIST "CAResAttic_BlastTrussMatAndCons" 1 0 0 0
;*************************************************************************
	"Blast Attic:TrussMATERIAL"		Attic:TrussMATERIAL		= {	if (LocalCompAssigned( TrussMATERIAL )) then  DeleteComp()  else  UNCHANGED  endif  }
	"Blast Attic:TrussLAYER"			Attic:TrussLAYER			= {	if (LocalCompAssigned( TrussLAYER    )) then  DeleteComp()  else  UNCHANGED  endif  }
	"Blast Attic:TrussCONS"				Attic:TrussCONS			= {	if (LocalCompAssigned( TrussCONS     )) then  DeleteComp()  else  UNCHANGED  endif  }
END
;*************************************************************************


