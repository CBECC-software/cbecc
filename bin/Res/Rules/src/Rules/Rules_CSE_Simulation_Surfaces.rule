;*************************************************************************
RULELIST "CSE_SimulationPrep_Surfaces" 1 0 0 1
;*************************************************************************
 ; CSE SURFACE creation & defaulting
	; UNDERGROUND WALLS
	"Create new SURFACE components"					UndWall:Action								= {	EvalRulelist( "Create_UndWall_SURFACEs", 1 )  }

	"Set UndWall:FrameSURFACE:Parent"				UndWall:FrameSURFACE:Parent			= {	Parent( CSE_ZONE )  }
	"Set UndWall:CavitySURFACE:Parent"				UndWall:CavitySURFACE:Parent			= {	Parent( CSE_ZONE )  }

	"Set UndWall:FrameSURFACE:SourceUndWall"		UndWall:FrameSURFACE:SourceUndWall	= {	Local( Name )  }
	"Set UndWall:CavitySURFACE:SourceUndWall"		UndWall:CavitySURFACE:SourceUndWall	= {	Local( Name )  }

	"Set UndWall:FrameSURFACE:sfCon"					UndWall:FrameSURFACE:sfCon				= {	if (LocalCompAssigned( Construction:cseFrmCONS ) == 0) then  UNDEFINED
																														else  LocalRef( Construction:cseFrmCONS )  endif  }
	"Set UndWall:CavitySURFACE:sfCon"				UndWall:CavitySURFACE:sfCon			= {	if (LocalCompAssigned( Construction:cseCavCONS ) == 0) then  UNDEFINED
																														else  LocalRef( Construction:cseCavCONS )  endif  }

	"Set UndWall:FrameSURFACE:sfArea"				UndWall:FrameSURFACE:sfArea			= {	Local( Area ) *      Local( FramingFactor )  }
	"Set UndWall:CavitySURFACE:sfArea"				UndWall:CavitySURFACE:sfArea			= {	Local( Area ) * (1 - Local( FramingFactor ))  }

	"Set UndWall:CavitySURFACE:sfType"				UndWall:CavitySURFACE:sfType			= {	"Wall"  }
	"Set UndWall:FrameSURFACE:sfType"				UndWall:FrameSURFACE:sfType			= {	"Wall"  }

	"Set UndWall:CavitySURFACE:sfExCnd"				UndWall:CavitySURFACE:sfExCnd			= {	"Ground"  }
	"Set UndWall:FrameSURFACE:sfExCnd"				UndWall:FrameSURFACE:sfExCnd			= {	"Ground"  }

	"Set UndWall:CavitySURFACE:sfDepthBG"			UndWall:CavitySURFACE:sfDepthBG		= {	if (IfValidAnd( DepthBelowGrade >= 0 )) then  DepthBelowGrade  else  UNDEFINED  endif  }
	"Set UndWall:FrameSURFACE:sfDepthBG"			UndWall:FrameSURFACE:sfDepthBG		= {	if (IfValidAnd( DepthBelowGrade >= 0 )) then  DepthBelowGrade  else  UNDEFINED  endif  }

;	"Set UndWall:CavitySURFACE:sfTilt"				UndWall:CavitySURFACE:sfTilt			= {	Local( Tilt )  }
;	"Set UndWall:FrameSURFACE:sfTilt"				UndWall:FrameSURFACE:sfTilt			= {	Local( Tilt )  }

;	"Set UndWall:CavitySURFACE:sfAzm"				UndWall:CavitySURFACE:sfAzm			= {	if (LocalStatus( OrientationValue ) > 0) then  Local( OrientationValue )  else  UNDEFINED  endif  }
;	"Set UndWall:FrameSURFACE:sfAzm"					UndWall:FrameSURFACE:sfAzm				= {	if (LocalStatus( OrientationValue ) > 0) then  Local( OrientationValue )  else  UNDEFINED  endif  }

;	"Set UndWall:CavitySURFACE:sfExAbs"				UndWall:CavitySURFACE:sfExAbs			= {	if (LocalStatus( Absorptivity ) > 0) then  Local( Absorptivity )  else  UNDEFINED  endif  }
;	"Set UndWall:FrameSURFACE:sfExAbs"				UndWall:FrameSURFACE:sfExAbs			= {	if (LocalStatus( Absorptivity ) > 0) then  Local( Absorptivity )  else  UNDEFINED  endif  }

	"Set UndWall:CavitySURFACE:sfModel"				UndWall:CavitySURFACE:sfModel			= {	"Forward_Difference"  }
	"Set UndWall:FrameSURFACE:sfModel"				UndWall:FrameSURFACE:sfModel			= {	"Forward_Difference"  }


	; INTERIOR WALLS
	"Create new SURFACE components"					IntWall:Action								= {	EvalRulelist( "Create_IntWall_SURFACEs", 1 )  }

	"Set IntWall:FrameSURFACE:Parent"				IntWall:FrameSURFACE:Parent			= {	Parent( CSE_ZONE )  }
	"Set IntWall:CavitySURFACE:Parent"				IntWall:CavitySURFACE:Parent			= {	Parent( CSE_ZONE )  }

	"Set IntWall:FrameSURFACE:SourceIntWall"		IntWall:FrameSURFACE:SourceIntWall	= {	Local( Name )  }
	"Set IntWall:CavitySURFACE:SourceIntWall"		IntWall:CavitySURFACE:SourceIntWall	= {	Local( Name )  }

	"Set IntWall:FrameSURFACE:sfCon"					IntWall:FrameSURFACE:sfCon				= {	if (LocalCompAssigned( Construction:cseFrmCONS ) == 0) then  UNDEFINED
																														else  LocalRef( Construction:cseFrmCONS )  endif  }
	"Set IntWall:CavitySURFACE:sfCon"				IntWall:CavitySURFACE:sfCon			= {	if (LocalCompAssigned( Construction:cseCavCONS ) == 0) then  UNDEFINED
																														else  LocalRef( Construction:cseCavCONS )  endif  }

;	"Set IntWall:FrameSURFACE:sfArea"				IntWall:FrameSURFACE:sfArea			= {	Local( Area ) *      Local( FramingFactor )  }
;	"Set IntWall:CavitySURFACE:sfArea"				IntWall:CavitySURFACE:sfArea			= {	Local( Area ) * (1 - Local( FramingFactor ))  }
; SAC 5/8/15 - replaced above w/ following to ensure that IntWall cavity & frame areas are based on the NetWallArea (not input area) to account for child Doors (issue 535)
	"Set IntWall:FrameSURFACE:sfArea"				IntWall:FrameSURFACE:sfArea			= {	                 Local( NetWallArea ) * Local( FramingFactor )  }
	"Set IntWall:CavitySURFACE:sfArea"				IntWall:CavitySURFACE:sfArea			= {	Local( Area ) - (Local( NetWallArea ) * Local( FramingFactor ))  }

	"Set IntWall:CavitySURFACE:sfType"				IntWall:CavitySURFACE:sfType			= {	"Wall"  }
	"Set IntWall:FrameSURFACE:sfType"				IntWall:FrameSURFACE:sfType			= {	"Wall"  }

	"Set IntWall:CavitySURFACE:sfTilt"				IntWall:CavitySURFACE:sfTilt			= {	90  }
	"Set IntWall:FrameSURFACE:sfTilt"				IntWall:FrameSURFACE:sfTilt			= {	90  }

	"Set IntWall:CavitySURFACE:sfAzm"				IntWall:CavitySURFACE:sfAzm			= {	0  }
	"Set IntWall:FrameSURFACE:sfAzm"					IntWall:FrameSURFACE:sfAzm				= {	0  }

;	"Set IntWall:CavitySURFACE:sfExAbs"				IntWall:CavitySURFACE:sfExAbs			= {	if (LocalStatus( Absorptivity ) > 0) then  Local( Absorptivity )  else  UNDEFINED  endif  }
;	"Set IntWall:FrameSURFACE:sfExAbs"				IntWall:FrameSURFACE:sfExAbs			= {	if (LocalStatus( Absorptivity ) > 0) then  Local( Absorptivity )  else  UNDEFINED  endif  }

	"Set IntWall:CavitySURFACE:sfModel"				IntWall:CavitySURFACE:sfModel			= {	"Forward_Difference"  }
	"Set IntWall:FrameSURFACE:sfModel"				IntWall:FrameSURFACE:sfModel			= {	"Forward_Difference"  }

  ; SAC 10/3/13 - added logic to model surfaces where IsPartySurface toggle On as Adiabatic
	"Set IntWall:CavitySURFACE:sfExCnd"				IntWall:CavitySURFACE:sfExCnd			= {	if ((IsPartySurface < 0.5 .OR. OtherSideModeled > 0.5) .AND. LocalCompAssigned( Outside )) then  "AdjZn"  else  "Adiabatic"  endif  }
	"Set IntWall:FrameSURFACE:sfExCnd"				IntWall:FrameSURFACE:sfExCnd			= {	if ((IsPartySurface < 0.5 .OR. OtherSideModeled > 0.5) .AND. LocalCompAssigned( Outside )) then  "AdjZn"  else  "Adiabatic"  endif  }

	"Set IntWall:CavitySURFACE:sfAdjZn"				IntWall:CavitySURFACE:sfAdjZn			= {	if ((IsPartySurface < 0.5 .OR. OtherSideModeled > 0.5) .AND. LocalCompAssigned( Outside )) then  LocalRef( Outside:CSE_ZONE )  else  UNDEFINED  endif  }
	"Set IntWall:FrameSURFACE:sfAdjZn"				IntWall:FrameSURFACE:sfAdjZn			= {	if ((IsPartySurface < 0.5 .OR. OtherSideModeled > 0.5) .AND. LocalCompAssigned( Outside )) then  LocalRef( Outside:CSE_ZONE )  else  UNDEFINED  endif  }


	; CATHEDRAL CEILINGS
	"Create new SURFACE components"		CathedralCeiling:Action				= {	EvalRulelist( "Create_CathedralCeiling_SURFACEs", 1 )  }

	"Set CathedralCeiling:FrameSURFACE:Parent"		CathedralCeiling:FrameSURFACE:Parent			= {	Parent( CSE_ZONE )  }
	"Set CathedralCeiling:CavitySURFACE:Parent"		CathedralCeiling:CavitySURFACE:Parent			= {	Parent( CSE_ZONE )  }

	"Set CathedralCeiling:FrameSURFACE:SourceCathClg"		CathedralCeiling:FrameSURFACE:SourceCathClg	= {	Local( Name )  }
	"Set CathedralCeiling:CavitySURFACE:SourceCathClg"		CathedralCeiling:CavitySURFACE:SourceCathClg	= {	Local( Name )  }

	"Set CathedralCeiling:FrameSURFACE:sfCon"			CathedralCeiling:FrameSURFACE:sfCon			= {	if (LocalCompAssigned( Construction:cseFrmCONS ) == 0) then  UNDEFINED
																																	else  LocalRef( Construction:cseFrmCONS )  endif  }
	"Set CathedralCeiling:CavitySURFACE:sfCon"		CathedralCeiling:CavitySURFACE:sfCon		= {	if (LocalCompAssigned( Construction:cseCavCONS ) == 0) then  UNDEFINED
																																	else  LocalRef( Construction:cseCavCONS )  endif  }

	"Set CathedralCeiling:FrameSURFACE:sfArea"		CathedralCeiling:FrameSURFACE:sfArea		= {	                 Local( NetWallArea ) * Local( FramingFactor )  }
	"Set CathedralCeiling:CavitySURFACE:sfArea"		CathedralCeiling:CavitySURFACE:sfArea		= {	Local( Area ) - (Local( NetWallArea ) * Local( FramingFactor ))  }

	"Set CathedralCeiling:FrameSURFACE:sfType"		CathedralCeiling:FrameSURFACE:sfType		= {	"Ceiling"  }
	"Set CathedralCeiling:CavitySURFACE:sfType"		CathedralCeiling:CavitySURFACE:sfType		= {	"Ceiling"  }

	"Set CathedralCeiling:FrameSURFACE:sfTilt"		CathedralCeiling:FrameSURFACE:sfTilt		= {	Local( RoofTilt )  }
	"Set CathedralCeiling:CavitySURFACE:sfTilt"		CathedralCeiling:CavitySURFACE:sfTilt		= {	Local( RoofTilt )  }

	"Set CathedralCeiling:FrameSURFACE:sfAzm"			CathedralCeiling:FrameSURFACE:sfAzm			= {	if (LocalStatus( OrientationValue ) > 0) then  OrientationValue  else  UNDEFINED  endif  }
	"Set CathedralCeiling:CavitySURFACE:sfAzm"		CathedralCeiling:CavitySURFACE:sfAzm		= {	if (LocalStatus( OrientationValue ) > 0) then  OrientationValue  else  UNDEFINED  endif  }

	"Set CathedralCeiling:FrameSURFACE:sfExAbs"		CathedralCeiling:FrameSURFACE:sfExAbs		= {	if (LocalStatus( RoofSolReflect ) > 0) then  max( 0, 1 - RoofSolReflect )  else  UNDEFINED  endif  }
	"Set CathedralCeiling:CavitySURFACE:sfExAbs"		CathedralCeiling:CavitySURFACE:sfExAbs		= {	if (LocalStatus( RoofSolReflect ) > 0) then  max( 0, 1 - RoofSolReflect )  else  UNDEFINED  endif  }

	"Set CathedralCeiling:FrameSURFACE:sfExEpsLW"	CathedralCeiling:FrameSURFACE:sfExEpsLW	= {	if (LocalStatus( RoofEmiss ) > 0) then  RoofEmiss  else  UNDEFINED  endif  }
	"Set CathedralCeiling:CavitySURFACE:sfExEpsLW"	CathedralCeiling:CavitySURFACE:sfExEpsLW	= {	if (LocalStatus( RoofEmiss ) > 0) then  RoofEmiss  else  UNDEFINED  endif  }

	"Set CathedralCeiling:FrameSURFACE:sfModel"		CathedralCeiling:FrameSURFACE:sfModel		= {	"Forward_Difference"  }
	"Set CathedralCeiling:CavitySURFACE:sfModel"		CathedralCeiling:CavitySURFACE:sfModel		= {	"Forward_Difference"  }


	; CEILINGS BELOW ATTIC
	"Create new SURFACE components"		CeilingBelowAttic:Action				= {	EvalRulelist( "Create_CeilingBelowAttic_SURFACEs", 1 )  }

	"Set CeilingBelowAttic:FrameSURFACE:Parent"		CeilingBelowAttic:FrameSURFACE:Parent		= {	Parent( CSE_ZONE )  }
	"Set CeilingBelowAttic:CavitySURFACE:Parent"		CeilingBelowAttic:CavitySURFACE:Parent		= {	Parent( CSE_ZONE )  }

	"Set CeilingBelowAttic:FrameSURFACE:SourceClgBlwAtc"		CeilingBelowAttic:FrameSURFACE:SourceClgBlwAtc	= {	Local( Name )  }
	"Set CeilingBelowAttic:CavitySURFACE:SourceClgBlwAtc"		CeilingBelowAttic:CavitySURFACE:SourceClgBlwAtc	= {	Local( Name )  }

	"Set CeilingBelowAttic:FrameSURFACE:sfCon"		CeilingBelowAttic:FrameSURFACE:sfCon		= {	if (LocalCompAssigned( Construction:cseFrmCONS ) == 0) then  UNDEFINED
																																	else  LocalRef( Construction:cseFrmCONS )  endif  }
	"Set CeilingBelowAttic:CavitySURFACE:sfCon"		CeilingBelowAttic:CavitySURFACE:sfCon		= {	if (LocalCompAssigned( Construction:cseCavCONS ) == 0) then  UNDEFINED
																																	else  LocalRef( Construction:cseCavCONS )  endif  }

;	"Set CeilingBelowAttic:FrameSURFACE:sfArea"		CeilingBelowAttic:FrameSURFACE:sfArea		= {	                 Local( Area ) * LocalRef( Construction:FramingFactor )  }
;	"Set CeilingBelowAttic:CavitySURFACE:sfArea"		CeilingBelowAttic:CavitySURFACE:sfArea		= {	Local( Area ) - (Local( Area ) * LocalRef( Construction:FramingFactor ))  }
; SAC 12/4/12 - reduce ceiling area by attic edge area
	"Set CeilingBelowAttic:FrameSURFACE:sfArea"		CeilingBelowAttic:FrameSURFACE:sfArea		= {	                           (Area - AtcEdg_TotArea) * LocalRef( Construction:FramingFactor )  }
	"Set CeilingBelowAttic:CavitySURFACE:sfArea"		CeilingBelowAttic:CavitySURFACE:sfArea		= {	(Area - AtcEdg_TotArea) - ((Area - AtcEdg_TotArea) * LocalRef( Construction:FramingFactor ))  }

	"Set CeilingBelowAttic:FrameSURFACE:sfType"		CeilingBelowAttic:FrameSURFACE:sfType		= {	"Ceiling"  }
	"Set CeilingBelowAttic:CavitySURFACE:sfType"		CeilingBelowAttic:CavitySURFACE:sfType		= {	"Ceiling"  }

	"Set CeilingBelowAttic:FrameSURFACE:sfExCnd"		CeilingBelowAttic:FrameSURFACE:sfExCnd		= {	if (LocalCompAssigned( AtticZone ) < 0.5) then  "Adiabatic"
																																	else if (LocalCompAssigned( AtticZone:CSE_ZONE ) < 0.5) then  "Adiabatic"  else  "AdjZn"  endif endif  }
	"Set CeilingBelowAttic:CavitySURFACE:sfExCnd"	CeilingBelowAttic:CavitySURFACE:sfExCnd	= {	FrameSURFACE:sfExCnd  }

	"Set CeilingBelowAttic:FrameSURFACE:sfAdjZn"		CeilingBelowAttic:FrameSURFACE:sfAdjZn		= {	if (EnumString( FrameSURFACE:sfExCnd  ) == "Adiabatic") then  UNDEFINED  else  AtticZone:CSE_ZONE  endif  }
	"Set CeilingBelowAttic:CavitySURFACE:sfAdjZn"	CeilingBelowAttic:CavitySURFACE:sfAdjZn	= {	if (EnumString( CavitySURFACE:sfExCnd ) == "Adiabatic") then  UNDEFINED  else  AtticZone:CSE_ZONE  endif  }

	"Set CeilingBelowAttic:FrameSURFACE:sfModel"		CeilingBelowAttic:FrameSURFACE:sfModel		= {	"Forward_Difference"  }
	"Set CeilingBelowAttic:CavitySURFACE:sfModel"	CeilingBelowAttic:CavitySURFACE:sfModel	= {	"Forward_Difference"  }


	; ATTIC EDGE CATHEDRAL CEILINGS
	"Create new WA Cathedral SURFACE components"		CeilingBelowAttic:Action				= {	if (AtcEdg_WAArea < 0.1) then  UNCHANGED
																															else	EvalRulelist( "Create_AtticEdgeWACathedralCeiling_SURFACEs", 1 )  endif  }
	"Create new WB Cathedral SURFACE components"		CeilingBelowAttic:Action				= {	if (AtcEdg_WBArea < 0.1) then  UNCHANGED
																															else	EvalRulelist( "Create_AtticEdgeWBCathedralCeiling_SURFACEs", 1 )  endif  }

	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:Parent"					CeilingBelowAttic:AtcEdg_WAFrameSURFACE:Parent				= {	Parent( CSE_ZONE )  }
	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:Parent"					CeilingBelowAttic:AtcEdg_WACavitySURFACE:Parent				= {	Parent( CSE_ZONE )  }
	"Set CeilingBelowAttic:AtcEdg_WBFrameSURFACE:Parent"					CeilingBelowAttic:AtcEdg_WBFrameSURFACE:Parent				= {	Parent( CSE_ZONE )  }
	"Set CeilingBelowAttic:AtcEdg_WBCavitySURFACE:Parent"					CeilingBelowAttic:AtcEdg_WBCavitySURFACE:Parent				= {	Parent( CSE_ZONE )  }

	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:SourceClgBlwAtc"		CeilingBelowAttic:AtcEdg_WAFrameSURFACE:SourceClgBlwAtc	= {	Local( Name )  }
	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:SourceClgBlwAtc"		CeilingBelowAttic:AtcEdg_WACavitySURFACE:SourceClgBlwAtc	= {	Local( Name )  }
	"Set CeilingBelowAttic:AtcEdg_WBFrameSURFACE:SourceClgBlwAtc"		CeilingBelowAttic:AtcEdg_WBFrameSURFACE:SourceClgBlwAtc	= {	Local( Name )  }
	"Set CeilingBelowAttic:AtcEdg_WBCavitySURFACE:SourceClgBlwAtc"		CeilingBelowAttic:AtcEdg_WBCavitySURFACE:SourceClgBlwAtc	= {	Local( Name )  }

	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfCon"		CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfCon	= {	if (LocalCompAssigned( AtcEdg_WACons:cseFrmCONS ) == 0) then  UNDEFINED
																																						else  LocalRef( AtcEdg_WACons:cseFrmCONS )  endif  }
	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfCon"		CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfCon	= {	if (LocalCompAssigned( AtcEdg_WACons:cseCavCONS ) == 0) then  UNDEFINED
																																						else  LocalRef( AtcEdg_WACons:cseCavCONS )  endif  }
	"Set CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfCon"		CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfCon	= {	if (LocalCompAssigned( AtcEdg_WBCons:cseFrmCONS ) == 0) then  UNDEFINED
																																						else  LocalRef( AtcEdg_WBCons:cseFrmCONS )  endif  }
	"Set CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfCon"		CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfCon	= {	if (LocalCompAssigned( AtcEdg_WBCons:cseCavCONS ) == 0) then  UNDEFINED
																																						else  LocalRef( AtcEdg_WBCons:cseCavCONS )  endif  }

	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfArea"		CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfArea	= {	                 AtcEdg_WAArea * LocalRef( AtcEdg_WACons:FramingFactor )  }
	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfArea"		CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfArea	= {	AtcEdg_WAArea - (AtcEdg_WAArea * LocalRef( AtcEdg_WACons:FramingFactor ))  }
	"Set CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfArea"		CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfArea	= {	                 AtcEdg_WBArea * LocalRef( AtcEdg_WBCons:FramingFactor )  }
	"Set CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfArea"		CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfArea	= {	AtcEdg_WBArea - (AtcEdg_WBArea * LocalRef( AtcEdg_WBCons:FramingFactor ))  }

	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfType"		CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfType	= {	"Ceiling"  }
	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfType"		CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfType	= {	"Ceiling"  }
	"Set CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfType"		CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfType	= {	"Ceiling"  }
	"Set CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfType"		CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfType	= {	"Ceiling"  }

; Can't specify Tilt unless we also specify Azimuth
;	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfTilt"		CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfTilt	= {	Attic:RoofTilt  }
;	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfTilt"		CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfTilt	= {	Attic:RoofTilt  }
;	"Set CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfTilt"		CeilingBelowAttic:AtcEdg_WBFrameSURFACE:sfTilt	= {	Attic:RoofTilt  }
;	"Set CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfTilt"		CeilingBelowAttic:AtcEdg_WBCavitySURFACE:sfTilt	= {	Attic:RoofTilt  }

; ADD OrientationValue ???
;	"Set CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfAzm"		CeilingBelowAttic:AtcEdg_WAFrameSURFACE:sfAzm		= {	if (LocalStatus( OrientationValue ) > 0) then  Local( OrientationValue )  else  UNDEFINED  endif  }
;	"Set CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfAzm"		CeilingBelowAttic:AtcEdg_WACavitySURFACE:sfAzm		= {	if (LocalStatus( OrientationValue ) > 0) then  Local( OrientationValue )  else  UNDEFINED  endif  }
