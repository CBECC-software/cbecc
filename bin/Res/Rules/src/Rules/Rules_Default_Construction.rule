	"Remaining construction, R-val & U-val calcs"	Cons:Action = {	EvalRulelist( "SetupCons_GenericFramed", 1 )  }

	"Set Cons:CodeMaximumUValue"		Cons:CodeMaximumUValue	= {	0  }
