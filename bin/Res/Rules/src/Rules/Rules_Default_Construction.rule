RULELIST "SetupCons_CathedralCeiling_WA"   0 0 0 1
;*************************************************************************
; INSIDE to OUTSIDE

	"Default Cons:UfactorCalcMethod"		Cons:UfactorCalcMethod	= {	1	}	; "Wood Framed Ceiling"      ->  "Parallel Path"
	"Set Cons:InsideLayerFirst"			Cons:InsideLayerFirst 	= {	0	}	; Cathedral Ceilings
	"Set Cons:Type"							Cons:Type				 	= {	10	}	; "Wood Framed Ceiling"  
	"Set Cons:NumPaths"						Cons:NumPaths 				= {	2	}	; "Wood Framed Ceiling"  
	"Set Cons:NumLayers"						Cons:NumLayers 			= {	7	}	; "Wood Framed Ceiling"  - attic edge WA

	"Set Cons:FrameCavityLayerMat[1]"	Cons:FrameCavityLayerMat[1] = {	     if (AtcEdg_ClgCons:InsideFinishLayer > 0.1 .AND. AtcEdg_ClgCons:SheathInsulLayer > 0.1) then  3
																								else if (AtcEdg_ClgCons:InsideFinishLayer > 0.1 .OR.  AtcEdg_ClgCons:SheathInsulLayer > 0.1) then  2
																								else  1  endif endif  }
	"Set Cons:FrameCavityLayerMat[2]"	Cons:FrameCavityLayerMat[2]	= {	0  }

   "Set Cons:AbvJoistInsulThick"		Cons:AbvJoistInsulThick		= {	0  }    ; = HB2 for WB construction
  ; SAC 1/13/13 - revised Improved multiplier to 1.0
  ; SAC 4/25/14 - revised rules to take into account ATTIC ROOF DECK insulation
   "Set Cons:ICQCeilingInsMult"		Cons:ICQCeilingInsMult		= {	if (IfValidAnd( AtcEdg_AtcCons:CavityLayer > 0.1 ))
																							then	switch (Proj:InsulConsQuality)
																										case 2 :  1.0						; 0.987 - (0.0017  * AtcEdg_ClgCons:CavityLayer)
																										default:  0.96  - (0.00347 * (AtcEdg_ClgCons:CavityLayer + (AtcEdg_AtcCons:CavityLayer / cos( (ValidOr( AtcEdg_Attic:RoofTilt, 0.166 ) * 3.14159265 / 180) ))))
																									endswitch
																							else	if (AtcEdg_ClgCons:CavityLayer < 0.1) then  0
																									else  switch (Proj:InsulConsQuality)
																												case 2 :  1.0						; 0.987 - (0.0017  * AtcEdg_ClgCons:CavityLayer)
																												default:  0.96  - (0.00347 * AtcEdg_ClgCons:CavityLayer)
																											endswitch  endif  endif  }
	"Set Cons:CavityMatCondQII[1]"	Cons:CavityMatCondQII[1]	= {	if (ICQCeilingInsMult < 0.0001 .OR. Proj:InsulRValPerInch[1] < 0.1) then  0
																							else if (IfValidAnd( AtcEdg_AtcCons:CavityLayer > 0.1 ))
																							then	(((AtcEdg_ClgCons:CavityLayer + (AtcEdg_AtcCons:CavityLayer / cos( (ValidOr( AtcEdg_Attic:RoofTilt, 0.166 ) * 3.14159265 / 180) ))) / Proj:InsulRValPerInch[1]) / 12) /
																									 ((AtcEdg_ClgCons:CavityLayer + (AtcEdg_AtcCons:CavityLayer / cos( (ValidOr( AtcEdg_Attic:RoofTilt, 0.166 ) * 3.14159265 / 180) ))) * ICQCeilingInsMult)
																							else if (AtcEdg_ClgCons:CavityLayer < 0.1) then  0
																							else   ((AtcEdg_ClgCons:CavityLayer / Proj:InsulRValPerInch[1]) / 12) / (AtcEdg_ClgCons:CavityLayer * ICQCeilingInsMult)
																							endif endif endif  }
	"Set Cons:CavityMatCondQII[2]"	Cons:CavityMatCondQII[2]	= {	CavityMatCondQII[1]  }
	"Set Cons:CavityMatCondQII[3]"	Cons:CavityMatCondQII[3]	= {	CavityMatCondQII[1]  }
	"Set Cons:CavityMatCond[1]"		Cons:CavityMatCond[1]		= {	if (Proj:InsulRValPerInch[1] < 0.1) then  0
																							else if (IfValidAnd( AtcEdg_AtcCons:CavityLayer > 0.1 ))
																							then	(((AtcEdg_ClgCons:CavityLayer + (AtcEdg_AtcCons:CavityLayer / cos( (ValidOr( AtcEdg_Attic:RoofTilt, 0.166 ) * 3.14159265 / 180) ))) / Proj:InsulRValPerInch[1]) / 12) /
																									  (AtcEdg_ClgCons:CavityLayer + (AtcEdg_AtcCons:CavityLayer / cos( (ValidOr( AtcEdg_Attic:RoofTilt, 0.166 ) * 3.14159265 / 180) )))
																							else if (AtcEdg_ClgCons:CavityLayer < 0.1) then  0
																							else  ((AtcEdg_ClgCons:CavityLayer / Proj:InsulRValPerInch[1]) / 12) / AtcEdg_ClgCons:CavityLayer
																							endif endif endif  }
	"Set Cons:CavityMatCond[2]"		Cons:CavityMatCond[2]		= {	CavityMatCond[1]  }
	"Set Cons:CavityMatCond[3]"		Cons:CavityMatCond[3]		= {	CavityMatCond[1]  }

