																									default :	SupplyDuctLoc      ; any other
																								endswitch  }
	"Set HVACDist:ReturnDuctLocUse"		HVACDist:ReturnDuctLocUse	= {	switch ( DuctLocation )
																									case  1 :	ReturnDuctAttic    ; "Attic"
																									default :	ReturnDuctLoc      ; any other
																								endswitch  }

