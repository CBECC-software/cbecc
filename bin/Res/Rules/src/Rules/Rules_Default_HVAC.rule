																										else if (HtSysAFUE > CA13HeatingEquipment:AFUE( "CodeBase", Proj:StdDesignBase, "StdVer", Proj:StandardsVersion, "SysType", HeatSysTypeAbbrev ))
																										then	 1		; "Better than Standard"
																										else	-1		; "Worse than Standard"
																										endif endif
																								else if (CA13HeatingEquipment:HSPFValid( "CodeBase", Proj:StdDesignBase, "StdVer", Proj:StandardsVersion, "SysType", HeatSysTypeAbbrev ) > 0 .AND.
																											LocalStatus( HPSysHSPF ) > 0)
																								then	if (HPSysHSPF == CA13HeatingEquipment:HSPF( "CodeBase", Proj:StdDesignBase, "StdVer", Proj:StandardsVersion, "SysType", HeatSysTypeAbbrev ))
																										then	 0		; "Equal to Standard"
																										else if (HPSysHSPF > CA13HeatingEquipment:HSPF( "CodeBase", Proj:StdDesignBase, "StdVer", Proj:StandardsVersion, "SysType", HeatSysTypeAbbrev ))
																										then	 1		; "Better than Standard"
																										else	-1		; "Worse than Standard"
																										endif endif
																								else			-1		; "Worse than Standard"
																								endif endif endif  }
END
